{
  "semi": true,
  "singleQuote": true,
  "tabWidth": 2,
  "useTabs": false,
  "trailingComma": "es5",
  "printWidth": 100,
  "bracketSpacing": true,
  "arrowParens": "always",
  "endOfLine": "lf",
  "jsxSingleQuote": false,
  "plugins": ["@ianvs/prettier-plugin-sort-imports", "prettier-plugin-tailwindcss"],
  "importOrder": [
    "^(react/(.*)$)|^(react$)",
    "^(react-dom/(.*)$)|^(react-dom$)",
    "<THIRD_PARTY_MODULES>",
    "",
    "^@/components(/(.*))?$",
    "^@/hooks(/(.*))?$",
    "^@/lib(/(.*))?$",
    "^@/store(/(.*))?$",
    "^@/types(/(.*))?$",
    "^@/styles(/(.*))?$",
    "",
    "^[./]"
  ]
}
